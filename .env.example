# Copy this file to .env and fill in your credentials before running
# Do NOT commit .env; it is ignored by .gitignore

# OpenAI API key for GPT-4 calls
OPENAI_API_KEY=your_openai_api_key_here

# Google Custom Search JSON API key and engine ID
GOOGLE_CSE_API_KEY=your_google_cse_api_key_here
GOOGLE_CSE_ENGINE_ID=your_google_cse_engine_id_here